{
  "name": "aster_lick_hunter_node",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/process-manager.js dev",
    "dev:direct": "concurrently --kill-others --kill-others-on-fail -n \"WEB,BOT\" -c \"blue,green\" \"npm run dev:web\" \"npm run dev:bot\"",
    "dev:web": "next dev --turbopack",
    "dev:bot": "tsx watch src/bot/index.ts",
    "build": "next build --turbopack",
    "start": "node scripts/process-manager.js start",
    "start:direct": "concurrently --kill-others --kill-others-on-fail -n \"WEB,BOT\" -c \"blue,green\" \"npm run start:web\" \"npm run start:bot\"",
    "start:web": "next start",
    "start:bot": "tsx src/bot/index.ts",
    "setup": "npm install && npm run build",
    "lint": "eslint",
    "bot": "tsx src/bot/index.ts",
    "bot:dev": "tsx watch src/bot/index.ts",
    "test": "tsx tests/test-limit-orders.ts",
    "test:simulation": "tsx tests/test-bot-simulation.ts",
    "test:flow": "tsx tests/test-limit-order-flow.ts",
    "test:all": "npm run test && npm run test:flow && npm run test:simulation"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.15.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
